# Google Cloud Configuration
# =========================
# 
# Copy this file to .env and fill in your values
# DO NOT commit .env file to version control

# Path to your Google Cloud service account key file
GOOGLE_APPLICATION_CREDENTIALS="/path/to/your/service-account-key.json"

# Google Cloud Project ID
GOOGLE_CLOUD_PROJECT="location-optimizer-1"

# BigQuery Configuration
BIGQUERY_PROJECT_ID="location-optimizer-1"
BIGQUERY_LOCATION="US"

# Data Collection Settings
WISCONSIN_COLLECTION_ENABLED="true"
ILLINOIS_COLLECTION_ENABLED="false"

# Logging Level
LOG_LEVEL="INFO"

# Sample API Keys (replace with real ones when available)
WISCONSIN_DFI_API_KEY="your_wisconsin_dfi_api_key_here"
SBA_API_KEY="your_sba_api_key_here"
MILWAUKEE_API_TOKEN="your_milwaukee_open_data_token_here"